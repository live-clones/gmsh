    GMSH_API double time()
    {
      int ierr = 0;
      double result_api_ = gmshLoggerTime(&ierr);
      if(ierr) throw ierr;
      return result_api_;
    }

    // Return CPU time.
    GMSH_API double cputime()
    {
      int ierr = 0;
      double result_api_ = gmshLoggerCputime(&ierr);
      if(ierr) throw ierr;
      return result_api_;
    }

